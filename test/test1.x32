.bounds

const my_const $F0 $20
:my_label db $FF my_const, 40

:s22 struct
  :a1 db 0
  :b1 db 0
  :st struct
    :a2 db 0
    :b2 db 0
  end
end

:init()
  :v = "a string now"
  sts v ("a string now")
  if 10 > $A0 & 20 > $02
    v = $FFFF - 30
  else
    v = ($FF + 30) ;this is a comment
  end

end

:shut()
end

:tick(ms)
end

:main()
  init()
  #1:to 10 2

  :i = 0
  whl @i < 5
    #1:bs
    i = @i + 1
    print(@i)
  end

  for :x 1, 5
    print(@x)
  end

  :n db $01 $01 $02 'F' '3' $00 $03 $10 $08 $14 $30 $00 $00
  :wad = #snd:note(n)
  #snd:play(wad)

  :stack = [100]
  stk stack 100
  psh stack 10 20 30
  print pop stack

  print "-->", #1 + 32 / 2
  print "-->", #vid + 32 / 2
  int_start("my_int", :(ms) print "interrupt" ms end, 1000)
  ;hlt()

end
